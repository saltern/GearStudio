;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Gear Studio"
run/main_scene="res://SCENES/MAIN/main.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/subwindows/embed_subwindows=false
window/stretch/aspect="ignore"

[dotnet]

project/assembly_name="Gear Studio"

[gui]

theme/custom="res://main_theme.tres"
theme/custom_font="res://FONTS/segoeui.ttf"
theme/default_font_antialiasing=2

[rendering]

environment/defaults/default_clear_color=Color(0.1, 0.1, 0.1, 1)
